        <div class="panel-heading">Pending Finish Product Requisition List</div>
        <div class="panel-body">
            <div class="col-md-2">
                <button ng-click="vm.editSelectedRow()" class="btn btn-primary" ng-disabled="vm.disableEdit" style="margin:10px 0 10px 0">Confirm Transit</button>
                @*<button ng-click="vm.displayFinishProductDetails()" class="btn btn-primary" ng-disabled="vm.disableEdit" style="margin:10px 0 10px 0">Display Detail</button>*@
            </div>
            <div class="col-md-12">
                <div id="productRequisitionListGrid" ui-grid="vm.gridOptions" ui-grid-selection class="uigridfull"></div>
            </div>
        </div>
    </div>
</div>